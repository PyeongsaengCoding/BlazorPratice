@page "/rantadi"
@using Newtonsoft.Json

<div class="d-flex flex-column bg-color-black h-100">
    <div class="d-flex flex-wrap">
        @foreach (var position in positions)
        {
            <button class="btn text-white"
                    @onclick="@(e => selectPosition(position))">
                @position
            </button>
        }
    </div>
    <div class="d-flex flex-column flex-wrap">
        @foreach (var unitByGrade in UnitListDict)
        {
            switch (unitByGrade.Key)
            {
                case "Nomal":
                    <div class="d-flex flex-wrap">
                        @foreach (var unit in unitByGrade.Value)
                        {
                            <button class="btn nomal-unit_color"
                                    @onclick="@(e => AddOwnUnitDict(unitByGrade.Key, unit))">
                                @unit
                            </button>
                        }
                    </div>


                    break;
                case "Magic":
                    <div class="d-flex flex-wrap">
                        @foreach (var unit in unitByGrade.Value)
                        {
                            <button class="btn magic-unit_color"
                                    @onclick="@(e => AddOwnUnitDict(unitByGrade.Key, unit))">
                                @unit
                            </button>
                        }
                    </div>
                    break;
                case "Rare":
                    <div class="d-flex flex-wrap">
                        @foreach (var unit in unitByGrade.Value)
                        {
                            <button class="btn rare-unit_color"
                                    @onclick="@(e => AddOwnUnitDict(unitByGrade.Key, unit))">
                                @unit
                            </button>
                        }
                    </div>
                    break;
                case "Unique":
                    <div class="d-flex flex-wrap">
                        @foreach (var unit in unitByGrade.Value)
                        {
                            <button class="btn unique-unit_color text-white"
                                    @onclick="@(e => AddOwnUnitDict(unitByGrade.Key, unit))">
                                @unit
                            </button>
                        }
                    </div>
                    break;
                case "Epic":
                    <div class="d-flex flex-wrap">
                        @foreach (var unit in unitByGrade.Value)
                        {
                            <button class="btn epic-unit_color"
                                    @onclick="@(e => AddOwnUnitDict(unitByGrade.Key, unit))">
                                @unit
                            </button>
                        }
                    </div>
                    break;
                default:
                    break;
            }
        }
    </div>

    <div class="mt-5">
        <span class="lead text-white">내가 가진 목록</span>
    </div>

    <div class="d-flex mb-4">
        @foreach (var unitByGrade in OwnUnitDict)
        {
            switch (unitByGrade.Key)
            {
                case "Nomal":
                    <div class="d-flex flex-wrap">
                        @foreach (var unit in unitByGrade.Value)
                        {
                            <button class="btn nomal-unit_color"
                                    @onclick="@(e => DeleteOwnUnitDict(unitByGrade.Key, unit))">
                                @unit
                            </button>
                        }
                    </div>


                    break;
                case "Magic":
                    <div class="d-flex flex-wrap">
                        @foreach (var unit in unitByGrade.Value)
                        {
                            <button class="btn magic-unit_color"
                                    @onclick="@(e => DeleteOwnUnitDict(unitByGrade.Key, unit))">
                                @unit
                            </button>
                        }
                    </div>
                    break;
                case "Rare":
                    <div class="d-flex flex-wrap">
                        @foreach (var unit in unitByGrade.Value)
                        {
                            <button class="btn rare-unit_color"
                                    @onclick="@(e => DeleteOwnUnitDict(unitByGrade.Key, unit))">
                                @unit
                            </button>
                        }
                    </div>
                    break;
                case "Unique":
                    <div class="d-flex flex-wrap">
                        @foreach (var unit in unitByGrade.Value)
                        {
                            <button class="btn unique-unit_color"
                                    @onclick="@(e => DeleteOwnUnitDict(unitByGrade.Key, unit))">
                                @unit
                            </button>
                        }
                    </div>
                    break;
                case "Epic":
                    <div class="d-flex flex-wrap">
                        @foreach (var unit in unitByGrade.Value)
                        {
                            <button class="btn epic-unit_color"
                                    @onclick="@(e => DeleteOwnUnitDict(unitByGrade.Key, unit))">
                                @unit
                            </button>
                        }
                    </div>
                    break;
                default:
                    break;
            }
        }
    </div>

    <div class="">
        <span class="lead text-white">미션 목록</span>
    </div>

    <div class="container">
        @foreach (var mission in cloneRantadiMissions)
        {
            <div class="row text-white">
                <div class="col-4 px-0">
                    <div class="d-flex flex-column justify-content-center h-100">
                        <span>미션명:  @mission.MissionName</span>
                    </div>
                </div>
                <div class="col-4 px-0">
                    <div class="d-flex flex-column justify-content-center h-100">
                        <span>@mission.MissionBenefit</span>
                    </div>
                </div>
                <div class="col-4 px-0">
                    <button class="btn btn-primary" @onclick="@(e => ToCompleteMission(mission))">미션 완료</button>
                </div>
            </div>
            <div class="row mb-4 text-white">
                <div class="col-2 nomal-unit_color">
                    @{
                        string nomalUnits = string.Join(", ", mission.NormalUnit);
                    }
                    <div class="d-flex flex-column justify-content-center h-100">
                        <span>@nomalUnits</span>
                    </div>
                </div>
                <div class="col-2 magic-unit_color">
                    @{
                        string magiUnits = string.Join(", ", mission.MagicUnit);
                    }
                    <div class="d-flex flex-column justify-content-center h-100">
                        <span>@magiUnits</span>
                    </div>
                </div>
                <div class="col-2 rare-unit_color">
                    @{
                        string rareUnits = string.Join(", ", mission.RarelUnit);
                    }
                    <div class="d-flex flex-column justify-content-center h-100">
                        <span>@rareUnits</span>
                    </div>
                </div>
                <div class="col-2 unique-unit_color">
                    @{
                        string uniqueUnits = string.Join(", ", mission.UniquelUnit);
                    }
                    <div class="d-flex flex-column justify-content-center h-100">
                        <span>@uniqueUnits</span>
                    </div>
                </div>
                <div class="col-2 epic-unit_color">
                    @{
                        string epicUnits = string.Join(", ", mission.EpiclUnit);
                    }
                    <div class="d-flex flex-column justify-content-center h-100">
                        <span>@epicUnits</span>
                    </div>
                </div>
                <div class="col-2">
                    @{
                        string citizen = string.Join(", ", mission.Citizen);
                    }
                    <div class="d-flex flex-column justify-content-center h-100">
                        <span>@citizen</span>
                    </div>
                </div>
            </div>
        }
    </div>
</div>



