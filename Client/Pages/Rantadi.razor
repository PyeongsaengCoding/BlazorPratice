    protected override async Task OnInitializedAsync()
    {
        await SetMissions();
    }

    private async Task AddOwnUnitDict(string grade, string unit)
    {
        if (OwnUnitDict.TryGetValue(grade, out List<string> units))
        {
            OwnUnitDict[grade].Add(unit);

            await CalculateMissionAtAdd(grade, unit);
        }
        else
        {
            OwnUnitDict.Add(grade, new()
{
                unit
            });

            await CalculateMissionAtAdd(grade, unit);
        }
    }

    private async Task DeleteOwnUnitDict(string grade, string unit)
    {
        if (OwnUnitDict.TryGetValue(grade, out List<string> units))
        {
            units.Remove(unit);

            await CalculateMissionAtDelete(grade, unit);
        };
    }

    private async Task CalculateMissionAtAdd(string grade, string unit)
    {
        if (grade.Contains("Nomal"))
        {
            foreach (var mission in cloneRantadiMissions)
            {
                mission.NormalUnit.Remove(unit);
            }
        }
        else if (grade.Contains("Magic"))
        {
            foreach (var mission in cloneRantadiMissions)
            {
                mission.MagicUnit.Remove(unit);
            }
        }
        else if (grade.Contains("Rare"))
        {
            foreach (var mission in cloneRantadiMissions)
            {
                mission.RarelUnit.Remove(unit);
            }
        }
        else if (grade.Contains("Unique"))
        {
            foreach (var mission in cloneRantadiMissions)
            {
                mission.UniquelUnit.Remove(unit);
            }
        }
        else if (grade.Contains("Epic"))
        {
            foreach (var mission in cloneRantadiMissions)
            {
                mission.EpiclUnit.Remove(unit);
            }
        }
    }

    private async Task CalculateMissionAtDelete(string grade, string unit)
    {
        if (grade.Contains("Nomal"))
        {
            foreach (var mission in rantadiMissions)
            {
                if (mission.NormalUnit.Contains(unit))
                {
                    var index = rantadiMissions.IndexOf(mission);
                    cloneRantadiMissions[index].NormalUnit.Add(unit);
                }
            }
        }
        else if (grade.Contains("Magic"))
        {
            foreach (var mission in rantadiMissions)
            {
                if (mission.MagicUnit.Contains(unit))
                {
                    var index = rantadiMissions.IndexOf(mission);
                    cloneRantadiMissions[index].MagicUnit.Add(unit);
                }
            }
        }
        else if (grade.Contains("Rare"))
        {
            foreach (var mission in rantadiMissions)
            {
                if (mission.RarelUnit.Contains(unit))
                {
                    var index = rantadiMissions.IndexOf(mission);
                    cloneRantadiMissions[index].RarelUnit.Add(unit);
                }
            }
        }
        else if (grade.Contains("Unique"))
        {
            foreach (var mission in rantadiMissions)
            {
                if (mission.UniquelUnit.Contains(unit))
                {
                    var index = rantadiMissions.IndexOf(mission);
                    cloneRantadiMissions[index].UniquelUnit.Add(unit);
                }
            }
        }
        else if (grade.Contains("Epic"))
        {
            foreach (var mission in rantadiMissions)
            {
                if (mission.EpiclUnit.Contains(unit))
                {
                    var index = rantadiMissions.IndexOf(mission);
                    cloneRantadiMissions[index].EpiclUnit.Add(unit);
                }
            }
        }
    }

    private async Task ToCompleteMission(RantadiMission rantadiMission)
    {
        var targetCloneRantadiMission = cloneRantadiMissions.FirstOrDefault(x => x.MissionName.Contains(rantadiMission.MissionName));
        cloneRantadiMissions.Remove(targetCloneRantadiMission);

        var targetRantadiMission = rantadiMissions.FirstOrDefault(x => x.MissionName.Contains(rantadiMission.MissionName));
        rantadiMissions.Remove(targetRantadiMission);
    }

    private async Task selectPosition(string selectedPosition)
    {
        foreach (var position in positions)
